    },
    {
      "speaker": "senpai-spooky",
      "speakerAnimation": "talk",
      "box": "roses-spooky",
      "boxAnimation": "speaking",

      "text": [
        "And hello children, What would your names be?"
      ]
    },
    {
      "speaker": "skid-pixel",
      "speakerAnimation": "talk",
      "box": "roses-spooky",
      "boxAnimation": "speaking",

      "text": [
        "I'm Skid!"
      ]
    },
    {
      "speaker": "pump-pixel",
      "speakerAnimation": "talk",
      "box": "roses-spooky",
      "boxAnimation": "speaking",

      "text": [
        "And I'm Pump!"
      ]
    },
    {
      "speaker": "skid-pixel",
      "speakerAnimation": "talk",
      "box": "roses-spooky",
      "boxAnimation": "speaking",

      "text": [
        "This is our friend, Moloch!"
